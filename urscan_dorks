# 1–10 Subdomains & DNS
domain:*.target.com
apexDomain:target.com
page.domain:*.target.com
domain:target.com AND NOT page.domain:target.com
domain:target.com AND page.ip:*
domain:target.com AND ip:*
domain:target.com AND ptr:*
domain:target.com AND page.country:*
domain:target.com AND page.city:*
domain:target.com AND page.asn:*

# 11–20 JavaScript & assets hunting
page.domain:target.com AND filename:".js"
page.domain:target.com AND filename:"main"
page.domain:target.com AND filename:"config"
page.domain:target.com AND filename:"bundle"
domain:target.com AND url:".js?*"
domain:target.com AND url:"/static/"
domain:target.com AND url:"/assets/"
domain:target.com AND url:"/cdn/"
domain:target.com AND mimeType:"application/javascript"
domain:target.com AND filename:".map"

# 21–30 API & endpoints
domain:target.com AND path:"/api"
