domain:target.com AND url:"endpoint"
domain:target.com AND url:"/oauth"
domain:target.com AND url:"/auth"
domain:target.com AND filename:"swagger"

# 31–40 Sensitive files
filename:".env" AND domain:target.com
filename:"config" AND domain:target.com
filename:"credentials" AND domain:target.com
filename:"secret" AND domain:target.com
filename:"backup" OR filename:".bak"
filename:"database" AND domain:target.com
url:"/debug" AND domain:target.com
url:"/internal" AND domain:target.com
domain:target.com AND url:"/test"
domain:target.com AND url:"/staging"

# 41–50 Vulnerable endpoints / params
url:"upload" AND domain:target.com
url:"download" AND domain:target.com
url:"redirect=" AND domain:target.com
url:"next=" AND domain:target.com
url:"callback=" AND domain:target.com
url:"return=" AND domain:target.com
url:"id=" AND domain:target.com
url:"file=" AND domain:target.com
url:"path=" AND domain:target.com
url:"/admin" AND domain:target.com
